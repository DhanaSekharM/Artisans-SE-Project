<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Background Color -->
    <solid android:color="@color/common_dark" />

    <!-- Border Color -->
    <stroke android:width="0.5dp"
        android:color="@color/common_dark" />

    <!-- Round Corners -->
    <corners android:radius="4dp" />

    <size
        android:height="200dp"
        android:width="200dp"></size>

    <padding
        android:top="8dp"
        android:bottom="8dp"
        android:left="8dp"
        android:right="8dp"
        ></padding>

</shape>